{
    "name": "Pomodoro Clock WebApp",
    "version": "0.0.1",
    "default_locale": "en",
    "homepage_url":"htp://pomodoro.lesson.im",
    "manifest_version": 2,
    "minimum_chrome_version": "23",
    "description": "Tiny pomodoro clock. Use any where.",
    "icons": {
        "16": "assets/icon.png",
        "128": "assets/icon.png"
    },
    "background": {
    // Recommended
    "persistent": false
    },
    "offline_enabled": true,
    "permissions": [
        "storage",
        "notifications",
        "<all_urls>"
    ]
}
